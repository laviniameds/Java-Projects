
public class Produto {
	
	private int codigo;
	private String descricao;
	private double preco;
	
	public Produto(int codigo, String descricao, double preco){
		this.codigo = codigo;
		this.descricao = descricao;
		this.preco = preco;
	}
	
	public int getCodigo(){
		return codigo;
	}
	
	public iString getDescricao(){
		return descricao;
	}
	
}
